{
  "name": "startfrom",
  "description": "Use any Github repo as a template for a new project.",
  "version": "1.1.7",
  "author": "Callum Locke",
  "babel": {
    "presets": [
      "es2015-node4"
    ]
  },
  "bin": {
    "startfrom": "cli.js"
  },
  "dependencies": {
    "bluebird": "^3.3.1",
    "chalk": "^1.1.1",
    "cross-spawn": "^2.1.5",
    "figures": "^1.4.0",
    "gunzip-maybe": "^1.3.1",
    "meow": "^3.7.0",
    "parse-github-url": "^0.3.0",
    "request": "^2.69.0",
    "rimraf": "^2.5.2",
    "sander": "^0.4.0",
    "shell-escape": "^0.2.0",
    "tar-fs": "^1.10.0",
    "update-notifier": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015-node4": "^2.0.3",
    "eslint": "^2.1.0",
    "eslint-config-trip": "^1.0.1",
    "eslint-plugin-babel": "^3.1.0",
    "mocha": "^2.4.5",
    "source-map-support": "^0.4.0"
  },
  "engines": {
    "node": ">=4",
    "npm": ">=2"
  },
  "eslintConfig": {
    "extends": "trip"
  },
  "files": [
    "dist/cli",
    "src/cli",
    "cli.js"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "callumlocke/startfrom",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist --source-maps",
    "buildwatch": "rimraf dist && babel src --out-dir dist --watch --source-maps",
    "prepublish": "npm run build",
    "test": "mocha dist/test",
    "testwatch": "cd dist && mocha --watch"
  }
}
