{
  "name": "startfrom",
  "description": "Use any Github repo as a template for a new project.",
  "version": "3.0.0",
  "author": "Callum Locke",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": 4
          }
        }
      ],
      "stage-0"
    ]
  },
  "bin": {
    "startfrom": "cli.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.22.0",
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "cross-spawn": "^5.0.1",
    "del": "^2.2.2",
    "figures": "^2.0.0",
    "gunzip-maybe": "^1.3.1",
    "input": "^1.0.1",
    "meow": "^3.7.0",
    "parse-github-url": "^1.0.0",
    "request": "^2.79.0",
    "sander": "^0.6.0",
    "shell-escape": "^0.2.0",
    "tar-fs": "^1.15.0",
    "update-notifier": "^1.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-env": "^1.1.8",
    "babel-preset-stage-0": "^6.22.0",
    "deep-eql": "^2.0.1",
    "del-cli": "^0.2.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "mocha": "^3.2.0",
    "source-map-support": "^0.4.11"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "dist/cli",
    "src/cli",
    "cli.js"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "callumlocke/startfrom",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "build:watch": "del dist && babel src --out-dir dist --watch --source-maps",
    "prepublish": "npm run build",
    "test": "eslint . && mocha dist/test",
    "test:watch": "cd dist && mocha --watch"
  }
}
