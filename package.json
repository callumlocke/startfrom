{
  "name": "startfrom",
  "description": "Use any Github repo as a template for a new project.",
  "version": "2.0.1",
  "author": "Callum Locke",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  },
  "bin": {
    "startfrom": "cli.js"
  },
  "dependencies": {
    "babel-polyfill": "^6.7.2",
    "bluebird": "^3.3.4",
    "chalk": "^1.1.1",
    "cross-spawn": "^2.1.5",
    "del": "^2.2.0",
    "figures": "^1.4.0",
    "gunzip-maybe": "^1.3.1",
    "input": "^1.0.1",
    "meow": "^3.7.0",
    "parse-github-url": "^0.3.0",
    "request": "^2.69.0",
    "sander": "^0.4.0",
    "shell-escape": "^0.2.0",
    "tar-fs": "^1.11.1",
    "update-notifier": "^0.6.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "deep-eql": "^0.1.3",
    "del-cli": "^0.2.0",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-babel": "^3.1.0",
    "mocha": "^2.4.5",
    "source-map-support": "^0.4.0"
  },
  "engines": {
    "node": ">=0.12"
  },
  "files": [
    "dist/cli",
    "src/cli",
    "cli.js"
  ],
  "license": "MIT",
  "main": "index.js",
  "repository": "callumlocke/startfrom",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "build:watch": "del dist && babel src --out-dir dist --watch --source-maps",
    "prepublish": "npm run build",
    "test": "eslint . && mocha dist/test",
    "test:watch": "cd dist && mocha --watch"
  }
}
